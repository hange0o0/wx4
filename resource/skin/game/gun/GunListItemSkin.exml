			<e:HorizontalLayout gap="2" verticalAlign="middle" horizontalAlign="center"/>
		</e:layout>
	</e:Group>
	<e:Button id="btn" label="升级" skinName="Btn1Skin" width="120" bottom="20" includeIn="unlock,levelUp" label.unlock="解锁" skinName.levelUp="Btn2Skin" right="15"/>
	<e:Label text="MAX" bold="true" right="40" verticalCenter="0" includeIn="max"/>
</e:Skin>