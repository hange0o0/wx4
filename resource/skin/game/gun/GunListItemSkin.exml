<?xml version="1.0" encoding="utf-8"?>
<e:Skin class="GunListItemSkin" width="580" height="140" xmlns:e="http://ns.egret.com/eui" xmlns:w="http://ns.egret.com/wing" states="unlock,levelUp,max,lock" >
	<e:Image source="item_png" scale9Grid="20,20,11,10" left="0" right="0" top="0" bottom="0" locked="true"/>
	<e:Image id="bg" source="black_bg_png" width="120" height="120" x="10" y="10"/>
	<e:Image id="mc" source="black_bg_png" width="120" height="120" x="10" y="10"/>
	<e:Label id="nameText" text="Label" y="15" size="26" x="150" bold="true"/>
	<e:Label id="atkText" text="攻击：1234   攻速：0.5秒" y="50" size="22" x="150" width="290" lineSpacing="6" anchorOffsetX="0" textColor="0xfcd79f"/>
	<e:Label id="desText" text="LabelLabelLabelLabelbelLabelLabel" y="80" size="22" x="150" width="280" lineSpacing="6" textColor="0xD6D6D6" anchorOffsetX="0"/>
